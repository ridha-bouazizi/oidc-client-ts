{
  "name": "oidc-redis-examples",
  "version": "1.0.0",
  "description": "Examples for oidc-client-ts with Redis server-side support",
  "main": "server-side-redis-integration.js",
  "type": "module",
  "scripts": {
    "setup": "node setup.js",
    "start": "node --loader ts-node/esm server-side-redis-integration.ts",
    "dev": "node --loader ts-node/esm --watch server-side-redis-integration.ts",
    "simple": "node --loader ts-node/esm simple-redis-example.ts",
    "server": "node demo-server.cjs",
    "test": "node test-setup.cjs",
    "test-redis": "node test-redis-state-store.cjs"
  },
  "dependencies": {
    "express": "^4.18.2",
    "redis": "^4.6.8",
    "cookie-parser": "^1.4.6",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cookie-parser": "^1.4.3",
    "@types/uuid": "^9.0.2",
    "@types/node": "^20.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
