    "nodejs",
    "distributed-auth"
  ],
  "author": {
    "name": "Ridha Bouazizi",
    "email": "contact.ridha.bouazizi@gmail.com"
  },
  "contributors": [
    {
      "name": "Original oidc-client-ts authors",
      "url": "https://github.com/authts/oidc-client-ts/graphs/contributors"
    }
  ],
  "peerDependencies": {
    "redis": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "redis": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
